
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Boards {
  id  Int @id @default(autoincrement())
  Name String?
  category String?
  author String?
   image  String?
   card Card[]
}
model Card{
  id Int @id @default(autoincrement())
  title String
  description String
  boardId Int
  author String?
  board Boards @relation(fields: [boardId], references: [id])
}

